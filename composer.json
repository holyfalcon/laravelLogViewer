{
    "name": "falcon/logviewer",
    "description": "organized logs",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Falcon\\Logviewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Falcon\\Logviewer\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "shahin",
            "email": "sh.bozorgi@phinix.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "psr/log": "^2.0 || ^3.0",
        "monolog/monolog": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.23|^7.0|^8.0",
        "phpunit/phpunit": "^9.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Falcon\\Logviewer\\LogviewerServiceProvider"
            ]
        }
    }
}
